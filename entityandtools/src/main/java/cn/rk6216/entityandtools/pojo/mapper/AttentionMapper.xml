<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.demo.AttentionMapper" >
  <resultMap id="BaseResultMap" type="com.demo.Attention" >
    <result column="user_id" property="userId" jdbcType="VARCHAR" />
    <result column="attention_list_id" property="attentionListId" jdbcType="VARCHAR" />
  </resultMap>
  <insert id="insert" parameterType="com.demo.Attention" >
    insert into attention (user_id, attention_list_id)
    values (#{userId,jdbcType=VARCHAR}, #{attentionListId,jdbcType=VARCHAR})
  </insert>
  <insert id="insertSelective" parameterType="com.demo.Attention" >
    insert into attention
    <trim prefix="(" suffix=")" suffixOverrides="," >
      <if test="userId != null" >
        user_id,
      </if>
      <if test="attentionListId != null" >
        attention_list_id,
      </if>
    </trim>
    <trim prefix="values (" suffix=")" suffixOverrides="," >
      <if test="userId != null" >
        #{userId,jdbcType=VARCHAR},
      </if>
      <if test="attentionListId != null" >
        #{attentionListId,jdbcType=VARCHAR},
      </if>
    </trim>
  </insert>
</mapper>